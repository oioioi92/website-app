{
  "name": "website-phase2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/prisma-generate-auto.cjs",
    "dev": "next dev --webpack",
    "prebuild": "node scripts/prisma-generate-auto.cjs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "prisma:generate": "prisma generate --schema prisma/schema.postgres.prisma",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema.postgres.prisma",
    "prisma:generate:pg": "prisma generate --schema prisma/schema.postgres.prisma",
    "prisma:generate:sqlite": "prisma generate --schema prisma/schema.sqlite.prisma",
    "migrate:pg": "prisma migrate dev --schema prisma/schema.postgres.prisma",
    "migrate:sqlite": "prisma db push --schema prisma/schema.sqlite.prisma",
    "seed:admin": "tsx scripts/seed-admin.ts",
    "reset:admin:password": "tsx scripts/reset-admin-password.ts",
    "import:mvp": "node scripts/import_mvp_json.js",
    "seed:demo": "tsx scripts/seed-demo-content.ts",
    "seed:test": "tsx scripts/seed-test-scenarios.ts",
    "seed:test:tsnode": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.scripts.json scripts/seed-test-scenarios.ts",
    "screenshots:capture": "tsx scripts/capture-screenshots.ts",
    "test:e2e": "tsx scripts/smoke-e2e-browser.ts",
    "smoke": "node scripts/smoke-check.ts",
    "test:mask": "tsx lib/utils/__tests__/mask.test.ts",
    "daily-upgrade": "powershell -ExecutionPolicy Bypass -File ./scripts/daily-upgrade.ps1",
    "daily-upgrade:apply": "powershell -ExecutionPolicy Bypass -File ./scripts/daily-upgrade.ps1 -ApplyUpdates",
    "pr:final:v3.1": "bash ./scripts/pr-final-check-v3.1.sh",
    "pr:final:v3.1:win": "powershell -ExecutionPolicy Bypass -File scripts/pr-final-check-v3.1.ps1"
  },
  "dependencies": {
