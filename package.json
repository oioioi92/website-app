    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "next": "^16.0.1",
    "otplib": "^13.3.0",
    "playwright-core": "^1.58.2",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^5.11.0",
    "sharp": "^0.34.5",
    "socket.io-client": "^4.8.3",
    "zod": "^4.1.5"
  },
