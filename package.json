    "@types/react-dom": "^19.1.9",
    "eslint": "^9.34.0",
    "eslint-config-next": "^16.0.1",
    "pngjs": "^7.0.0",
    "prisma": "^6.14.0",
    "tailwindcss": "^4.1.12",
